alias php="$command run -w /app/${project_id} --rm php${php_version} php"
alias npm="$command run -w /app/${project_id} --rm npm npm"
alias composer="$command run -w /app/${project_id} --rm php${php_version} composer"

alias art="php artisan"
alias p="art test --parallel --processes 6 --exclude-group TBD,demo,legacyImports"
alias pf="art test --fi"
